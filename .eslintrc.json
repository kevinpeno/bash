{
	"env": {
		"es6": true
	},

	"ecmaFeatures": {
		"arrowFunctions": true,
		"binaryLiterals": true,
		"blockBindings": true,
		"classes": true,
		"defaultParams": false,
		"destructuring": false,
		"forOf": true,
		"generators": true,
		"modules": false,
		"objectLiteralComputedProperties": true,
		"objectLiteralDuplicateProperties": true,
		"objectLiteralShorthandMethods": true,
		"objectLiteralShorthandProperties": true,
		"octalLiterals": true,
		"regexUFlag": false,
		"regexYFlag": false,
		"spread": true,
		"superInFunctions": true,
		"templateStrings": true,
		"unicodeCodePointEscapes": false,
		"globalReturn": false,
		"jsx": false
	},

	"rules": {
		"no-extra-parens": 0,
		"valid-jsdoc": 0,
		"no-unexpected-multiline": 0,
		"accessor-pairs": 0,
		"block-scoped-var": 0,
		"complexity": 0,
		"dot-notation": 0,
		"guard-for-in": 0,
		"no-div-regex": 0,
		"no-else-return": 0,
		"no-eq-null": 0,
		"no-implicit-coercion": 0,
		"no-invalid-this": 0,
		"no-process-env": 0,
		"no-useless-call": 0,
		"no-void": 0,
		"no-warning-comments": 0,
		"vars-on-top": 0,
		"init-declarations": 0,
		"no-undefined": 0,
		"callback-return": 0,
		"handle-callback-err": 0,
		"no-mixed-requires": 0,
		"no-new-require": 0,
		"no-path-concat": 0,
		"no-restricted-modules": 0,
		"no-sync": 0,
		"array-bracket-spacing": 0,
		"camelcase": 0,
		"computed-property-spacing": 0,
		"consistent-this": 0,
		"func-names": 0,
		"func-style": 0,
		"id-length": 0,
		"indent": [2,"tab"],
		"lines-around-comment": 0,
		"linebreak-style": [2,"unix"],
		"max-nested-callbacks": 0,
		"new-cap": 0,
		"newline-after-var": 0,
		"no-inline-comments": 0,
		"no-nested-ternary": 2,
		"no-ternary": 0,
		"no-unneeded-ternary": 1,
		"object-curly-spacing": 0,
		"one-var": [2,"never"],
		"operator-assignment": 0,
		"padded-blocks": 0,
		"quote-props": [2,"always"],
		"id-match": 0,
		"sort-vars": 0,
		"space-before-blocks": 0,
		"space-in-parens": 0,
		"spaced-comment": 0,
		"arrow-parens": 0,
		"arrow-spacing": [2,{"before":true,"after":true}],
		"constructor-super": 2,
		"no-class-assign": 2,
		"no-const-assign": 2,
		"no-this-before-super": 2,
		"no-var": 2,
		"object-shorthand": [2,"always"],
		"prefer-const": 1,
		"prefer-spread": 1,
		"prefer-reflect": 0,
		"require-yield": 0,
		"max-depth": 0,
		"max-len": 0,
		"max-params": 0,
		"max-statements": 0,
		"no-bitwise": 0,
		"no-plusplus": 2,
		"comma-dangle": [2,"never"],
		"no-cond-assign": [2,"always"],
		"no-console": 1,
		"no-constant-condition": 1,
		"no-control-regex": 2,
		"no-debugger": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": [2,"functions"],
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-typeof": 2,
		"consistent-return": 2,
		"curly": [2,"multi-line"],
		"default-case": 2,
		"dot-location": [2,"property"],
		"eqeqeq": 2,
		"no-alert": 2,
		"no-caller": 2,
		"no-eval": 2,
		"no-extend-native": [1],
		"no-extra-bind": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-multi-spaces": [2],
		"no-multi-str": 2,
		"no-native-reassign": [2],
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-new": 2,
		"no-octal-escape": 2,
		"no-octal": 2,
		"no-param-reassign": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 1,
		"no-sequences": 2,
		"no-throw-literal": 2,
		"no-unused-expressions": 2,
		"no-with": 2,
		"radix": 1,
		"wrap-iife": 2,
		"yoda": [2,"never",{"exceptRange":true}],
		"strict": [2,"global"],
		"no-catch-shadow": 2,
		"no-delete-var": 2,
		"no-label-var": 2,
		"no-shadow-restricted-names": 2,
		"no-shadow": 2,
		"no-undef-init": 2,
		"no-undef": 2,
		"no-unused-vars": 2,
		"no-use-before-define": 2,
		"no-process-exit": 2,
		"brace-style": [2,"stroustrup",{"allowSingleLine":true}],
		"comma-spacing": [2,{"after":true}],
		"comma-style": [2,"last"],
		"eol-last": 2,
		"key-spacing": [2,{"beforeColon":false,"afterColon":true}],
		"new-parens": 2,
		"no-array-constructor": 2,
		"no-continue": 2,
		"no-lonely-if": 2,
		"no-mixed-spaces-and-tabs": [2,false],
		"no-multiple-empty-lines": [2,{"max":1}],
		"no-new-object": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 2,
		"no-underscore-dangle": 2,
		"operator-linebreak": [2,"after"],
		"quotes": [2,"double"],
		"semi-spacing": [2,{"before":false,"after":true}],
		"semi": [2,"never"],
		"keyword-spacing": ["error"],
		"space-before-function-paren": [2,{"anonymous":"never","named":"never"}],
		"space-infix-ops": 2,
		"space-unary-ops": [2,{"words":true,"nonwords":false}],
		"wrap-regex": 2,
		"generator-star-spacing": [2,"before"]
	}
}
